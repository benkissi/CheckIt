
	<div class="" style="margin-top:10px; padding-right:20px;">

		<% if manufacturer_signed_in? %>
		  Logged in as <strong><%= current_manufacturer.email %></strong>.
		  <%= link_to 'Edit profile', edit_manufacturer_registration_path, :class => 'navbar-link' %> |
		  <%= button_to "Logout", destroy_manufacturer_session_path, method: :delete, :class => 'navbar-link', class: "log" %>
		<% else %>
		  <%= link_to "Sign up", new_manufacturer_registration_path, :class => 'navbar-link'  %> |
		  <%= link_to "Login", new_manufacturer_session_path, :class => 'navbar-link'  %>
		<% end %>
	</div>

