<div class="table-bg1">
  <div class="form-content container">
    

    <!-- <div class="bg-opaq"> </div> -->
    <div class="table-pad">
    <%= render 'login' %>
      <h1>Listing Reports</h1>
      <div class="table-responsive">
      <table class="table table-responsive  table-back table-hover table-context table-bordered form-center " >
        <thead>
          <tr class="info ">
            <th>Pharmacy name</th>
            <th>Pharmacy location</th>
            <th>Pharmacy landmark</th>
            <th>Additional information</th>
            <th>Number</th>
            <th>Email</th>
            <th>Date created</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        
        <tbody>
          <% @report.each do |report| %>
            <tr>
              <td><%= report.pharmacy_name %></td>
              <td><%= report.pharmacy_location %></td>
              <td><%= report.pharmacy_landmark %></td>
              <td><%= report.additional_info %></td>
              <td><%= report.number %></td>
              <td><%= report.email %></td>
              <td><%= report.created_at %></td>
              <td><%= link_to 'Show', report %></td>
              <td><%= link_to 'Destroy', report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
              <td><%= link_to 'product',report.product%></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    
  </div>
</div>
</div>

<script>
 $(function() {
  $('#footer').removeClass("top-space");
 });
 </script>